class test_maxcomdiv{

int maxcomdiv (int a, int b){
  int dividendo,divisor,resto;
	dividendo = a;
	divisor = b;
	resto = 1;
	while (((((resto != 0))))){

		resto += 1;
		dividendo -= divisor;
		continue;
	}

	return dividendo;
}

void main(){
    externinvk("printf", float , "%d Mcd",  maxcomdiv(6,10 ) );
    return;
}

}
